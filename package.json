    "tailwindcss-animate": "^1.0.7"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "serve": "vite preview",
    "lint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx && tsc",
